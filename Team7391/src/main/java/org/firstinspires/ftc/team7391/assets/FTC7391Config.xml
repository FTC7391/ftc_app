        <Motor name="motor_right" port="2" />
    </MotorController>
    <MotorController name="Motor Controller Lift A" serialNumber="AL00VGUM">
        <Motor name="stage0" port="1" />
        <Motor name="stage1" port="2" />
    </MotorController>
    <MotorController name="Motor Controller Lift B" serialNumber="AL00XTK5">
        <Motor name="stage2" port="1" />
        <Motor name="stage3" port="2" />
